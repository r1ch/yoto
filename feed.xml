---
layout: null
---

<?xml version='1.0' encoding='utf-8'?>
<rss version='2.0' xmlns:content='http://purl.org/rss/1.0/modules/content/' xmlns:itunes='http://www.itunes.com/dtds/podcast-1.0.dtd'>
    <channel>
        <title>Friday Night Comedy</title>
        <link>{{ '/' | absolute_url }}</link>
        {% for episode in site.episodes %}
        <item>
            <title>{{ episode.relative_path}}</title>
            <itunes:explicit>Yes</itunes:explicit>
            <link>{{ episode.path | absolute_url }}</link>
            <itunes:duration>{{ 5 | xml_escape }}</itunes:duration>
            <enclosure url='{{ episode.path | absolute_url }}' length='{{ 5 | xml_escape }}'/>
        </item>
        {% endfor %}
    </channel>
</rss>
